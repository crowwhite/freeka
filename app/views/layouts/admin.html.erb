<!DOCTYPE html>
<html>
<head>
  <title>Freeka</title>
  <%= stylesheet_link_tag    'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='left'>
    <%= link_to 'Home', admins_welcome_path %>
  </div>
  <div id='head'>
    <%= render 'shared/users/sign_up_in' %>
  </div>
  <% flash.each do |type, message| %>
    <p class=<%= type %> >
      <%= message %>
    </p>
  <% end %>
  <div class='inline'>
    <%= yield %>
  </div>

</body>
</html>
